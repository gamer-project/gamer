Jet_Radius                           0.5                    # radius of the cylinder-shape jet source               (kpc)
Jet_HalfHeight                       0.5                    # half height of the cylinder-shape jet source          (kpc)
Jet_SrcVel                           120                    # jet velocity                    (in unit of speed of light)
Jet_SrcDens                          0.01                   # jet density                                        (g/cm^3)
Jet_SrcTemp                          1.00                   # jet temperature                                       (keV)
Jet_Cone_Vec_x                       1.0                    # precession direction x
Jet_Cone_Vec_y                       0.0                    # precession direction y
Jet_Cone_Vec_z                       0.0                    # precession direction z
Jet_CenOffset_x                     +0.0                    # jet central coordinates offset x                      (kpc) 
Jet_CenOffset_y                     +0.0                    # jet central coordinates offset y                      (kpc)
Jet_CenOffset_z                     +0.0                    # jet central coordinates offset z                      (kpc)
Jet_Angular_Velocity                +30.0                   # precession angular velocity                    (degree/kyr)
Jet_Angle                             30                    # precession angle                                   (degree)


# suddenly increase fluid quantities in the period between 'Jet_BurstStartTime' and 'Jet_BurstEndTime'
Flag_BurstDens                        0                     # suddendly increase density          [1=enabled, 0=disabled]
Flag_Burst4Vel                        0                     # suddendly increase 4-velocity       [1=enabled, 0=disabled]
Flag_BurstTemp                        0                     # suddendly increase temperature      [1=enabled, 0=disabled]


Jet_BurstDens                         0                     # peak density                                       (g/cm^3)
