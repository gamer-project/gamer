reset
save_encoding = GPVAL_ENCODING
set termoption enhanced
set title '{/=20 Relative Error of Mass Density'
set term png size 1000,460
set encoding utf8
set key bottom

set output 'Mass_Gas_RErr.png
set xlabel '{/=20 Step}' 
set ylabel '{/=20 Mass\_Gas\_RErr}'
plot 'Record__Conservation' using 2:5 ti''

# X-Momemtum
set output 'MomX_Gas_RErr.png
set title '{/=20 Relative Error of x-Momentum Density'
set ylabel '{/=20 MomX\_Gas\_RErr}'
plot 'Record__Conservation' using 2:8 ti''

# Y-Momemtum
set output 'MomY_Gas_RErr.png
set title '{/=20 Relative Error of y-Momentum Density'
set ylabel '{/=20 MomY\_Gas\_RErr}'
plot 'Record__Conservation' using 2:11 ti''

# Z-Momemtum
set output 'MomZ_Gas_RErr.png
set title '{/=20 Relative Error of z-Momentum Density'
set ylabel '{/=20 MomZ\_Gas\_RErr}'
plot 'Record__Conservation' using 2:14 ti''

set output 'Etot_Gas_RErr.png
set title '{/=20 Relative Error of Energy Density'
set ylabel '{/=20 Etot\_Gas\_RErr}'
plot 'Record__Conservation' using 2:23 ti''
